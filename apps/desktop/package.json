{
  "name": "desktop",
  "version": "1.0.0",
  "private": true,
  "main": "dist/main.js",
  "homepage": "./",
  "scripts": {
    "dev": "pnpm run build:electron && pnpm run dev:electron & pnpm run dev:react",
    "dev:electron": "electron .",
    "dev:react": "vite",
    "build": "pnpm run build:react && pnpm run build:electron",
    "build:react": "vite build",
    "build:electron": "tsc --project tsconfig.main.json",
    "start": "electron .",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "electron": "^27.0.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.8"
  }
}